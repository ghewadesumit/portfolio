     </Container>\n            <img className=\"background-image-left\" src={colorSharp} alt=\"Image\" />\n        </section>\n    )\n}\n","/Users/Sumit.A.Ghewade/Desktop/React/myportfolio/client/src/components/Projects.js",["74"],[],"import { Container, Row, Col, Tab } from \"react-bootstrap\";\nimport { ProjectCard } from \"./ProjectCard\";\nimport colorSharp2 from \"../assets/img/color-sharp2.png\";\nimport 'animate.css';\nimport TrackVisibility from 'react-on-screen';\nimport {projectDescription, projects} from './projectData';\n\n\nexport const Projects = () => {\n\n   return (\n        <section className=\"project\" id=\"project\">\n          <Container>\n            <Row>\n              <Col size={12}>\n                <TrackVisibility>\n                  {({ isVisible }) =>\n                  <div className={isVisible ? \"animate__animated animate__fadeIn\": \"\"}>\n                    <h2>Projects</h2>\n                    <p>{projectDescription} </p>\n                    <Tab.Container id=\"projects-tabs\" defaultActiveKey=\"first\">\n                          <Row>\n                            {\n                              projects.map((project, index) => {\n                                return (\n                                  <ProjectCard\n                                    key={index}\n                                    {...project}\n                                    />\n                                )\n                              })\n                            }\n                          </Row>\n                    </Tab.Container>\n                  </div>}\n                </TrackVisibility>\n              </Col>\n            </Row>\n          </Container>\n          <img className=\"background-image-right\" src={colorSharp2}></img>\n        </section>\n      )\n}","/Users/Sumit.A.Ghewade/Desktop/React/myportfolio/client/src/components/Banner.js",["75","76"],[],"import {useEffect, useState} from 'react';\nimport {Container,Row,Col} from 'react-bootstrap';\nimport {ArrowRightCircle} from 'react-bootstrap-icons';\nimport 'animate.css';\nimport headerImg from \"../assets/img/header-img.svg\";\nimport TrackVisibility from 'react-on-screen';\nimport {aboutme} from './projectData';\n\nexport const Banner = ({contactRef})=>{\n    const period = 350;\n    const deletingPeriod = 100;\n    const [loopNum,setLoopNum] = useState(0);\n    const [isDeleting, setIsDeleting] = useState(false);\n    const [delta,setDelta] = useState(period);\n    const [text,setText] = useState('');\n    const toRotate = ['Web Developer', \"Software Developer\", \"Full-Stack Developer\"];\n    const [index, setIndex] = useState(1);\n\n\n    useEffect(()=>{\n        let ticker = setInterval(()=>{\n            tick()\n        },delta)\n\n        return ()=>{ clearInterval(ticker)};\n    },[text]);\n\n    const tick = ()=>{\n        \n        let i = loopNum % toRotate.length;\n        let fullText = toRotate[i];\n        let updatedText = isDeleting ? fullText.substring(0,text.length-1) : fullText.substring(0,text.length + 1);\n\n        setText(updatedText);\n\n        if(isDeleting){\n            setDelta(deletingPeriod);\n        }\n\n        if(!isDeleting && updatedText === fullText){\n            setIsDeleting(true);\n            setDelta(period);\n        } else if(isDeleting && updatedText === ''){\n            setIsDeleting(false);\n            setLoopNum(loopNum + 1);\n            setIndex(1);\n            setDelta(period);\n          } else {\n            setIndex(prevIndex => prevIndex + 1);\n          }\n    }\n\n    return (\n        <section className=\"banner\" id=\"home\">\n        <Container>\n          <Row className=\"aligh-items-center\">\n            <Col xs={12} md={6} xl={7}>\n              <TrackVisibility>\n                {({ isVisible }) =>\n                <div className={isVisible ? \"animate__animated animate__fadeIn\" : \"\"}>\n                  {/* <span className=\"tagline\">Welcome to my Portfolio</span> */}\n                  <h1>{`Hi! I'm Sumit`} </h1>\n                  <div className=\"profile\">\n                    <h1><span className=\"txt-rotate\" data-rotate='[ \"Web Developer\", \"Web Designer\", \"UI/UX Designer\" ]'><span className=\"wrap\">{text}</span></span></h1>\n                  </div>\n                  <div className=\"aboutme\">\n                    <p>{aboutme}</p>\n                    <button onClick={()=> contactRef.current?.scrollIntoView({behavior: 'smooth'})}>Let’s Connect <ArrowRightCircle size={25} /></button>\n                  </div>\n                </div>}\n              </TrackVisibility>\n            </Col>\n            <Col xs={12} md={6} xl={5}>\n              <TrackVisibility>\n                {({ isVisible }) =>\n                  <div className={isVisible ? \"animate__animated animate__zoomIn\" : \"\"}>\n                    <img src={headerImg} alt=\"Header Img\"/>\n                  </div>}\n              </TrackVisibility>\n            </Col>\n          </Row>\n        </Container>\n      </section>\n    )\n}","/Users/Sumit.A.Ghewade/Desktop/React/myportfolio/client/src/components/Footer.js",[],[],"/Users/Sumit.A.Ghewade/Desktop/React/myportfolio/client/src/components/Modal.js",["77"],[],"import {useState} from 'react';\nimport Button from 'react-bootstrap/Button';\nimport Modal from 'react-bootstrap/Modal';\n\nexport const ModalEmail = ({message,show,emailSent,handleShowModal})=>{\n\n\n    const handleClose = () => handleShowModal(false,'');\n\n      return (\n        <Modal show={show} onHide= {handleClose} style={{color:\"black\"}} centered>\n        <Modal.Dialog >\n          <Modal.Header closeButton>\n            <Modal.Title>{emailSent ? 'Thanks for reaching out' : 'Ohh Noo!!!'}</Modal.Title>\n          </Modal.Header>\n    \n          <Modal.Body >\n           {message}\n          </Modal.Body>\n    \n          <Modal.Footer>\n            <Button onClick={handleClose} variant=\"secondary\">Close</Button>\n          </Modal.Footer>\n        </Modal.Dialog>\n        </Modal>\n      );\n    \n    \n}","/Users/Sumit.A.Ghewade/Desktop/React/myportfolio/client/src/components/Navbar.js",[],[],"/Users/Sumit.A.Ghewade/Desktop/React/myportfolio/client/src/components/Contact.js",["78","79"],[],"import { useState ,useRef} from \"react\";\nimport { Container, Row, Col } from \"react-bootstrap\";\nimport contactImg from \"../assets/img/contact-img.svg\";\nimport 'animate.css';\nimport TrackVisibility from 'react-on-screen';\nimport emailjs from '@emailjs/browser';\n\nexport const Contact = ({handleShowModal,contactRef}) => {\n  const formInitialDetails = {\n    firstName: '',\n    lastName: '',\n    email: '',\n    phone: '',\n    message: ''\n  }\n\n  const form = useRef();\n\n  const [formDetails, setFormDetails] = useState(formInitialDetails);\n  const [buttonText, setButtonText] = useState('Send');\n  const [status, setStatus] = useState({});\n\n  const onFormUpdate = (category, value) => {\n      setFormDetails({\n        ...formDetails,\n        [category]: value\n      })\n  }\n\n\n  const sendEmail = (e) => {\n    e.preventDefault();\n    // console.log(formDetails);\n    const sender = formDetails.firstName;\n    emailjs.sendForm('service_rtynb9j', 'template_t2i6k8r',form.current , 'fFl_6CBl0du1-BbOJ')\n      .then((result) => {\n          console.log(result.text);\n          handleShowModal(true,true,`Hey ${sender}, I will get in touch with you soon. `);\n      }, (error) => {\n          handleShowModal(true,false,`Hey ${sender}, There is some issue with this. Please reach me out on LinkedIn. Thanks!`);\n          console.log(error.text);\n      });\n\n    setFormDetails(formInitialDetails);\n  };\n\n  return (\n    <section ref={contactRef} className=\"contact\" id=\"connect\">\n      <Container>\n        <Row className=\"align-items-center\">\n          <Col size={12} md={6}>\n            <TrackVisibility>\n              {({ isVisible }) =>\n                <img className={isVisible ? \"animate__animated animate__zoomIn\" : \"\"} src={contactImg} alt=\"Contact Us\"/>\n              }\n            </TrackVisibility>\n          </Col>\n          <Col size={12} md={6}>\n            <TrackVisibility>\n              {({ isVisible }) =>\n                <div className={isVisible ? \"animate__animated animate__fadeIn\" : \"\"}>\n                <h2>Get In Touch</h2>\n                <form ref ={form} onSubmit={sendEmail}>\n                  <Row>\n                    <Col size={12} sm={6} className=\"px-1\">\n                      <input type=\"text\" value={formDetails.firstName} name=\"firstName\" placeholder=\"First Name\" onChange={(e) => onFormUpdate('firstName', e.target.value)} />\n                    </Col>\n                    <Col size={12} sm={6} className=\"px-1\">\n                      <input type=\"text\" value={formDetails.lasttName} name=\"lastName\" placeholder=\"Last Name\" onChange={(e) => onFormUpdate('lastName', e.target.value)}/>\n                    </Col>\n                    <Col size={12} sm={6} className=\"px-1\">\n                      <input type=\"email\" value={formDetails.email} name=\"email\" placeholder=\"Email Address\" onChange={(e) => onFormUpdate('email', e.target.value)} />\n                    </Col>\n                    <Col size={12} sm={6} className=\"px-1\">\n                      <input type=\"tel\" value={formDetails.phone} name=\"phone\" placeholder=\"Phone No.\" onChange={(e) => onFormUpdate('phone', e.target.value)}/>\n                    </Col>\n                    <Col size={12} className=\"px-1\">\n                      <textarea rows=\"6\" value={formDetails.message} name=\"message\" placeholder=\"Message\" onChange={(e) => onFormUpdate('message', e.target.value)}></textarea>\n                      <button type=\"submit\"><span>{buttonText}</span></button>\n                    </Col>\n                    {\n                      status.message &&\n                      <Col>\n                        <p className={status.success === false ? \"danger\" : \"success\"}>{status.message}</p>\n                      </Col>\n                    }\n                  </Row>\n                </form>\n              </div>}\n            </TrackVisibility>\n          </Col>\n        </Row>\n      </Container>\n    </section>\n  )\n}\n","/Users/Sumit.A.Ghewade/Desktop/React/myportfolio/client/src/components/SkillPercentage.js",[],[],"/Users/Sumit.A.Ghewade/Desktop/React/myportfolio/client/src/components/SocialIcons.js",[],[],"/Users/Sumit.A.Ghewade/Desktop/React/myportfolio/client/src/components/ProjectCard.js",["80"],[],"import { Col } from \"react-bootstrap\";\nimport Button from 'react-bootstrap/Button';\n\nexport const ProjectCard = ({ title, description,projectUrl, isProjectLive, imgUrl }) => {\n  return (\n    <Col size={12} sm={6} md={4}>\n      < div className=\"proj-imgbx\">\n        <img className=\"projectcard-img\" src={imgUrl} />\n        <div className=\"proj-txtx\">\n          <h4>{title}</h4>\n          <span>{description} </span>\n          {isProjectLive && <Button variant=\"success\" className=\"view-project\">   <a href={projectUrl} > View Project</a></Button>}\n          </div>\n      </div>\n    </Col>\n  )\n}\n","/Users/Sumit.A.Ghewade/Desktop/React/myportfolio/client/src/components/projectData.js",[],[],{"ruleId":"81","severity":1,"message":"82","line":49,"column":13,"nodeType":"83","endLine":49,"endColumn":83},{"ruleId":"84","severity":1,"message":"85","line":40,"column":11,"nodeType":"83","endLine":40,"endColumn":69},{"ruleId":"86","severity":1,"message":"87","line":17,"column":12,"nodeType":"88","messageId":"89","endLine":17,"endColumn":17},{"ruleId":"90","severity":1,"message":"91","line":26,"column":7,"nodeType":"92","endLine":26,"endColumn":13,"suggestions":"93"},{"ruleId":"86","severity":1,"message":"94","line":1,"column":9,"nodeType":"88","messageId":"89","endLine":1,"endColumn":17},{"ruleId":"86","severity":1,"message":"95","l